fun makeLexer yyinput =
let	val yygone0=1
	val yyb = ref "\n" 		(* buffer *)
	val yybl = ref 1		(*buffer length *)
	val yybufpos = ref 1		(* location of next character to use *)
	val yygone = ref yygone0	(* position in file of beginning of buffer *)
	val yydone = ref false		(* eof found yet? *)
	val yybegin = ref 1		(*Current 'start state' for lexer *)

	val YYBEGIN = fn (Internal.StartStates.STARTSTATE x) =>
		 yybegin := x

fun lex () : Internal.result =
let fun continue() = lex() in
  let fun scan (s,AcceptingLeaves : Internal.yyfinstate list list,l,i0) =
	let fun action (i,nil) = raise LexError
	| action (i,nil::l) = action (i-1,l)
	| action (i,(node::acts)::l) =
		case node of
		    Internal.N yyk => 
			(let fun yymktext() = String.substring(!yyb,i0,i-i0)
			     val yypos = i0+ !yygone
			open UserDeclarations Internal.StartStates
 in (yybufpos := i; case yyk of 

			(* Application actions *)

  1 => (lineNum := !lineNum+1; linePos := yypos :: !linePos; continue())
| 100 => (Tokens.OF(yypos,yypos+2))
| 104 => (Tokens.NIL(yypos,yypos+3))
| 11 => (Tokens.RPAREN(yypos,yypos+1))
| 110 => (Tokens.BREAK(yypos,yypos+5))
| 114 => (Tokens.FOR(yypos,yypos+3))
| 118 => (Tokens.VAR(yypos,yypos+3))
| 121 => (Tokens.TO(yypos,yypos+2))
| 13 => (Tokens.LBRACK(yypos,yypos+1))
| 130 => (Tokens.FUNCTION(yypos,yypos+8))
| 132 => (YYBEGIN STRING; continue())
| 134 => (YYBEGIN INITIAL; continue())
| 136 => let val yytext=yymktext() in Tokens.STRING(yytext, yypos, yypos+size(yytext)) end
| 141 => let val yytext=yymktext() in Tokens.ID(yytext, yypos, yypos+size(yytext)) end
| 144 => let val yytext=yymktext() in Tokens.INT(valOf (Int.fromString(yytext)),yypos,yypos+size(yytext)) end
| 147 => (YYBEGIN COMMENT; continue())
| 15 => (Tokens.RBRACK(yypos,yypos+1))
| 150 => (YYBEGIN INITIAL; continue())
| 152 => (continue())
| 155 => let val yytext=yymktext() in ErrorMsg.error yypos ("unclosed comments " ^ yytext); continue() end
| 157 => let val yytext=yymktext() in ErrorMsg.error yypos ("illegal character " ^ yytext); continue() end
| 17 => (Tokens.LBRACE(yypos,yypos+1))
| 19 => (Tokens.RBRACE(yypos,yypos+1))
| 21 => (Tokens.DOT(yypos,yypos+1))
| 23 => (Tokens.PLUS(yypos,yypos+1))
| 25 => (Tokens.MINUS(yypos,yypos+1))
| 27 => (Tokens.TIMES(yypos,yypos+1))
| 29 => (Tokens.DIVIDE(yypos,yypos+1))
| 3 => (Tokens.COMMA(yypos,yypos+1))
| 31 => (Tokens.LT(yypos,yypos+1))
| 33 => (Tokens.GT(yypos,yypos+1))
| 35 => (Tokens.AND(yypos,yypos+1))
| 37 => (Tokens.OR(yypos,yypos+1))
| 39 => (Tokens.EQ(yypos,yypos+1))
| 42 => (Tokens.NEQ(yypos,yypos+1))
| 45 => (Tokens.LE(yypos,yypos+1))
| 48 => (Tokens.GE(yypos,yypos+1))
| 5 => (Tokens.COLON(yypos,yypos+1))
| 51 => (Tokens.ASSIGN(yypos,yypos+1))
| 53 => (continue())
| 59 => (Tokens.WHILE(yypos,yypos+5))
| 63 => (Tokens.LET(yypos,yypos+3))
| 66 => (Tokens.IN(yypos,yypos+2))
| 7 => (Tokens.SEMICOLON(yypos,yypos+1))
| 70 => (Tokens.END(yypos,yypos+3))
| 75 => (Tokens.TYPE(yypos,yypos+4))
| 81 => (Tokens.ARRAY(yypos,yypos+5))
| 84 => (Tokens.IF(yypos,yypos+2))
| 89 => (Tokens.THEN(yypos,yypos+4))
| 9 => (Tokens.LPAREN(yypos,yypos+1))
| 94 => (Tokens.ELSE(yypos,yypos+4))
| 97 => (Tokens.DO(yypos,yypos+2))
